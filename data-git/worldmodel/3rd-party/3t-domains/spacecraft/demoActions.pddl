(in-package :ap)
(NEW-PREDICATE '(TASKS-DONE1 ?S1 ?S2 - SPACE-CRAFT) SPACECRAFT-DOMAIN)
(DEFINE (DURATIVE-ACTION DO-JOBS) :PARAMETERS (?S1 ?S2 - UNMANNED-SPACE-CRAFT) :VARS NIL :CONDITION
           (AND (AT START (NOT (= ?S1 ?S2)))) :EXPANSION
           (PARALLEL (POINTED&IMAGED ?S1 JUPITER) (POINTED&IMAGED ?S2 MARS)) :EFFECT
           (AT END (TASKS-DONE1 ?S1 ?S2)))
(DEFINE (PROBLEM DONE-JOBS) (:DOMAIN SPACECRAFT-DOMAIN) (:SITUATION SPACECRAFT1) (:DEADLINE 390.0)
           (:GOAL (TASKS-DONE1 CASSINI DAWN)))


